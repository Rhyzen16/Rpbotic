void setup(){

  pinMode(1, OUTPUT);
  pinMode(2, OUTPUT);
  pinMode(3, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(5, OUTPUT);
  
}


void loop(){
 digitalWrite(1, HIGH);  //BLUE_when initialize 11
  digitalWrite(2, HIGH); //YELLOW_when initialize 12
   digitalWrite(3, HIGH); //GREEN_when initialize 13
  digitalWrite(4, HIGH); //GREEN_when initialize 13
   digitalWrite(5, HIGH); //GREEN_when initialize 13
delay(222); 

   digitalWrite(1, HIGH);  //BLUE_when initialize 11
  digitalWrite(2, HIGH); //YELLOW_when initialize 12
   digitalWrite(3, HIGH); //GREEN_when initialize 13
  digitalWrite(4, HIGH); //GREEN_when initialize 13
   digitalWrite(5, HIGH); //GREEN_when initialize 13
delay(222); 

   digitalWrite(1, HIGH);  //BLUE_when initialize 11
  digitalWrite(2, HIGH); //YELLOW_when initialize 12
   digitalWrite(3, HIGH); //GREEN_when initialize 13
  digitalWrite(4, HIGH); //GREEN_when initialize 13
   digitalWrite(5, HIGH); //GREEN_when initialize 13
delay(222); 
}
